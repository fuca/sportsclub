    <div class="col-md-4">
	{control commonMenu:grid, "club", "systemModule.protected.rootPage.clubMenuHeading"}
    </div>
    <div class="col-md-4">
	{control adminMenu:grid, "admin", "systemModule.protected.rootPage.adminMenuHeading"}
    </div>
</div>