{/block}
    {block #title}
	<li>{_ "systemModule.default.signpost.bread"}</li> {$titleCrumbSeparator} {include #parent}
    {/block}
    
    {block #side}
    right
    {/block}
    
    {block #sideBlock}
    {/block}

{block #mainBlock}

<div class="row">
